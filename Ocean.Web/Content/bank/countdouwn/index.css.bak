
#content>.page{
	position:relative;
	margin:40px auto 0px auto;
	width:620px;
	padding:15px;
	display:table;
	border-radius:5px;
	background-color:#FFFFFF;
	box-shadow:0px 0px 6px rgba(0, 0, 0, .1);
}
#content>.page:first-child{
	margin:0px auto 0px auto;
}

#content>.page>.countdown{
	padding:0px;
	margin:0px;
	width:715px;
	border-radius:4px;
	text-align:center;
	display:table-cell;
	vertical-align:middle;
}
#content>.page>.pagetitle{
	position:relative;
	z-index:1;
	margin:-20px;
	left:25%;
	text-align:center;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size:30px;
	color:#FFF;
	font-weight: bold;
}
#content>.page>.countdown.background1{
	background-color:#000000;
	background:linear-gradient(top,#000000,#404040);
	background:-moz-linear-gradient(top,#000000,#404040);		/* Firefox */
	background:-webkit-linear-gradient(top,#000000,#404040);	/* Webkit */
	background:-o-linear-gradient(top,#000000,#404040);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#000000', endColorstr='#404040')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#000000", endColorstr="#404040");			/* IE */
}

#content>.page>.backgroundPicker{
	position:absolute;
	z-index:1;
	padding:3px 6px 6px 6px;
	margin:0px;
	bottom:20px;
	left:20px;
	text-align:right;
}
#content>.page>.backgroundPicker .button{
	position:relative;
	padding:0px;
	margin:0px 0px 0px 2px;
	float:left;
	width:15px;
	height:15px;
	cursor:pointer;
    display:inline-block;
    *zoom:1;						/* IE */
    *display:inline;				/* IE */
	border-radius:3px;
	border:1px solid rgba(0, 0, 0, .3);
	visibility:hidden;
}
#content>.page>.backgroundPicker .button:first-child{
	margin:0px 0px 0px 0px;
}
#content>.page>.backgroundPicker .button:hover{
	top:-1px;
}
#content>.page>.backgroundPicker .button:active{
	top:0px;
}
#content>.page>.backgroundPicker .button.selected{
	border:1px solid rgba(0, 0, 0, .8);
}
#content>.page>.backgroundPicker .button.black{
	background-color:#333333;
	background:linear-gradient(top,#555555,#333333);
	background:-moz-linear-gradient(top,#555555,#333333);		/* Firefox */
	background:-webkit-linear-gradient(top,#555555,#333333);	/* Webkit */
	background:-o-linear-gradient(top,#555555,#333333);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#555555', endColorstr='#333333')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#555555", endColorstr="#333333");			/* IE */
	box-shadow:0px 1px 0px rgba(0, 0, 0, .1),
	0px 1px 0px rgba(255, 255, 255, .1) inset;
}
#content>.page>.backgroundPicker .button.white{
	background-color:#EEEEEE;
	background:linear-gradient(top,#EEEEEE,#BBBBBB);
	background:-moz-linear-gradient(top,#EEEEEE,#BBBBBB);		/* Firefox */
	background:-webkit-linear-gradient(top,#EEEEEE,#BBBBBB);	/* Webkit */
	background:-o-linear-gradient(top,#EEEEEE,#BBBBBB);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#EEEEEE', endColorstr='#BBBBBB')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#EEEEEE", endColorstr="#BBBBBB");			/* IE */
	box-shadow:0px 1px 0px rgba(0, 0, 0, .1),
	0px 1px 0px rgba(255, 255, 255, 1) inset;
}
#content>.page>.backgroundPicker .button.blue{
	background-color:#248AB2;
	background:linear-gradient(top,#50BCE6,#248AB2);
	background:-moz-linear-gradient(top,#50BCE6,#248AB2);		/* Firefox */
	background:-webkit-linear-gradient(top,#50BCE6,#248AB2);	/* Webkit */
	background:-o-linear-gradient(top,#50BCE6,#248AB2);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#50BCE6', endColorstr='#248AB2')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#50BCE6", endColorstr="#248AB2");			/* IE */
	box-shadow:0px 1px 0px rgba(0, 0, 0, .1),
	0px 1px 0px rgba(255, 255, 255, .3) inset;
}
#content>.page>.backgroundPicker .button.orange{
	background-color:#FFC800;
	background:linear-gradient(top,#FFC800,#FFA900);
	background:-moz-linear-gradient(top,#FFC800,#FFA900);		/* Firefox */
	background:-webkit-linear-gradient(top,#FFC800,#FFA900);	/* Webkit */
	background:-o-linear-gradient(top,#FFC800,#FFA900);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFC800', endColorstr='#FFA900')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#FFC800", endColorstr="#FFA900");			/* IE */
	box-shadow:0px 1px 0px rgba(0, 0, 0, .1),
	0px 1px 0px rgba(255, 255, 255, .3) inset;
}
#content>.page>.backgroundPicker .button.red{
	background-color:#FF8095;
	background:linear-gradient(top,#FF8095,#FF4160);
	background:-moz-linear-gradient(top,#FF8095,#FF4160);		/* Firefox */
	background:-webkit-linear-gradient(top,#FF8095,#FF4160);	/* Webkit */
	background:-o-linear-gradient(top,#FF8095,#FF4160);			/* Opera */
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF8095', endColorstr='#FF4160')";	/* IE9 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr="#FF8095", endColorstr="#FF4160");			/* IE */
	box-shadow:0px 1px 0px rgba(0, 0, 0, .1),
	0px 1px 0px rgba(255, 255, 255, .3) inset;
}
