@using Ocean.Framework.Configuration.global.config
@{
    string author = GlobalConfig.GetConfig()["Author"];
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
@section Head{
    <title>@(ViewBag.Title ?? "石狮农商银行")</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0,  minimum-scale=1.0, maximum-scafle=1.0, user-scalable=no" />
    <meta name="author" content="@(author)" />
    <script type="text/javascript" src="@Url.Content("~/Content/jquery-easyui-1.3.5/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/bank/js/option.js")"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bank/css/common.css")" />
    <script type="text/javascript">
        $(function () {
            $(".goback").click(function () {
                var refer = document.referrer;
                if (refer) {
                    history.go(-1);
                }
                else {
                    location.href = "/";
                }
            });
        });
    </script>
}
<body>
<div id="mbSubTitle">
    <section class="subTitleBox">
        <span>机具维护人员</span>
    </section>
</div>

<div id="frameRegisterMobile" class="frameLoginBox frameLoginChang">
          <section class="modBaseBox">
              <div class="jjtxt">
                 <div class="logo"><img src="@Url.Content("~/content/images/operator.jpg")" /></div>
                 <div style="text-align:center;">
                    <p>POS机具专业维护员:汤镇文</p>
                    <p>服务电话:4000896336</p>
                 </div>
              </div> 
          </section>
</div>
</body>
</html>