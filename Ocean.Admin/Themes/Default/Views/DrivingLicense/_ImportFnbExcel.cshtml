@using Ocean.Framework.Caching.Cache
@using Ocean.Entity.Enums
@using Ocean.Entity
@{
    ViewBag.Title = "福农宝客户导入";
} 
@model Funongbao
@section Head{
    <style type="text/css">
        td
        {
            height: 25px;
            line-height: 25px;
        }
    </style>
    <script type="text/javascript">
    @if (ViewBag.Method=="post"&&ViewBag.Success)
    {
        @:parent.closeImport("成功导入了@(ViewBag.CountFnb)个新福农宝客户，更新了@(ViewBag.CountFnbUpdate)个福农宝客户");
    }
    </script>
}
<div class="easyui-layout" data-options="fit:true">
    <div data-options="region:'center',border:false" style="padding: 10px;">
<form action="_ImportFnbExcel" id="_ImportFnbExcel" method="post" enctype="multipart/form-data">
        <table style="width: 100%;"><tr>
            <td style="width: 80%;">
                请选择要改入的Excel文件：
            </td>
            </tr>
            <tr>
                <td style="width: 80%;">
                    <input type="file" name="fnbExcel" id="fnExcel" />
                    <input type="submit" onclick="this.value='导入中……';this.disabled=true;document.getElementById('_ImportFnbExcel').submit();" value="导入客户" />
                </td>
            </tr>
            @if(ViewBag.Method=="post"&&!ViewBag.Success)
            {
            <tr>
                <td style="color:red;width: 80%;">
                    @ViewBag.Message
                </td>
            </tr>
            }
        </table>
</form>
    </div> 
</div>